<h1>All Posts</h1>
<button>
  <%= link_to "Post a Question!", new_question_path %>
</button>
<% @questions.each do |question| %>

    <div class="media">
        <div class="media-body">
            <h4 class="media-heading">
                <%= link_to question.title, question %>
            </h4>
        </div>
    </div>
    <% end %>
